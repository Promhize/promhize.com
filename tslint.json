{
  "extends": [
    "tslint-config-standard",
    "tslint-react"
  ],
  "rules": {
    "no-unused-variable": [true, {"ignore-pattern": "^_"}],
    "jsx-no-multiline-js": false,
    "quotemark": [true, "single", "jsx-single"],
    "trailing-comma": [true, {
    "multiline": {
      "objects": "always",
      "arrays": "always",
      "functions": "never",
      "typeLiterals": "ignore"
    },
    "esSpecCompliant": true
    }]
  }
}